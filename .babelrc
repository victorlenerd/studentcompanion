{
  "presets": ["react-native"],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    ["@babel/plugin-transform-runtime",{
      "polyfill": false,
      "regenerator": false
    }],
    ["module-resolver", {
      "cwd": "babelrc",
      "root": ["./src"],
      "alias": {
        "containers": "./src/containers",
        "pages": "./src/pages",
        "ducks": "./src/ducks",
        "components": "./src/components",
        "shared": "./src/shared"
      },
      "extensions": [".js", ".ios.js", ".android.js", ".json"]
    }]
  ]
}
