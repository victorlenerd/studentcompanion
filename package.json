{
  "name": "StudentCompanion",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native bundle --dev false  --assets-dest ./android/app/src/main/res/ --entry-file ./index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --platform android",
    "ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "clean:ios": "cd ios && xcodebuild -workspace 'StudentCompanion.xcworkspace' -scheme 'StudentCompanion' clean",
    "build:ios": "cd ios && xcodebuild -workspace 'StudentCompanion.xcworkspace' -scheme 'StudentCompanion' build",
    "clean:android": "cd android && ./gradlew clean",
    "build:android": "cd android && ./gradlew build",
    "build:apk": "cd android && ./gradlew assembleRelease",
    "pod:clean": "cd ios && pod cache clean --all",
    "pod:install": "cd ios && pod install",
    "rninstall": "rm -rf node_modules && yarn install",
    "test": "jest",
    "nodify": "./node_modules/.bin/rn-nodeify --hack --install",
    "formatter": "prettier --single-quote --trailing-comma=es5 --print-width=120 --write \"src/**/*.{js,jsx}\""
  },
